---
import { Image } from "@astrojs/image/components";

// Import components
import NavMenu from "./NavMenu";
const links = [
  { linkText: "Home", link: "/" },
  { linkText: "About", link: "/about" },
  { linkText: "Articles", link: "/articles" },
  { linkText: "Resources", link: "/resources" },
];

interface Props {
  className?: string;
}
const { className } = Astro.props;
---

<header class={(className ? className : "") + " bg-primary px-edge relative"}>
  <div class="mx-auto flex max-w-screen-2xl items-center justify-between py-4">
    <a href="/">
      <!-- <Image
        src={}
        alt="hundred million logo"
      /> -->
    </a>

    <NavMenu links={links} client:load />
  </div>
</header>
